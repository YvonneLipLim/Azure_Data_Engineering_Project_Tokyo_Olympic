<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CSV Viewer</title>
</head>
<body>
    <h1>CSV Data</h1>
    <table id="csv-table" border="1"></table>

    <script>
        fetch(https://github.com/YvonneLipLim/Azure_Data_Engineering_Project_Tokyo_Olympic/blob/main/Datasets/Athletes.csv')
            .then(response => response.text())
            .then(data => {
                const rows = data.split('\n');
                let html = '';
                rows.forEach(row => {
                    html += `<tr><td>${row.split(',').join('</td><td>')}</td></tr>`;
                });
                document.getElementById('csv-table').innerHTML = html;
            });
    </script>
</body>
</html>
